{
    "name": "corneltek/getoptionkit",
    "homepage": "http://github.com/c9s/GetOptionKit",
    "description": "Powerful command-line option toolkit",
    "license": "MIT",
    "authors": [
        {
            "name": "Yo-An Lin",
            "email": "yoanlin93@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "cweagans/composer-patches": "^1.7",
        "phpunit/phpunit": "^7.0",
        "phpunit/php-token-stream": "dev-3.x-php800 as 3.1.3"
    },
    "autoload": {
        "psr-4": {
            "GetOptionKit\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6.x-dev"
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "phpunit/phpunit": {
                "https://github.com/Sweetchuck/phpunit/pull/2 - phpunit-7.x - Support for PHP 8.1": "https://patch-diff.githubusercontent.com/raw/Sweetchuck/phpunit/pull/2.patch"
            }
        }
    }
}
